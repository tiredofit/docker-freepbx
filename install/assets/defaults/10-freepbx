ENABLE_XMPP=${ENABLE_XMPP:-"FALSE"}
FOP_DIRECTORY=${FOP_DIRECTORY:-"/fop"}
FREEPBX_VERSION=${FREEPBX_VERSION:-"15.0.16.56"}
HTTP_PORT=${HTTP_PORT:-80}
HTTPS_PORT=${HTTPS_PORT:-443}
